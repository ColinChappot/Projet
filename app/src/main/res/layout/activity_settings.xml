<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.colin.projet.SettingsActivity">

    <EditText
        android:id="@+id/editTextLoginW"
        android:layout_width="117dp"
        android:layout_height="32dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="159dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <EditText
        android:id="@+id/editTextPassowrdW"
        android:layout_width="117dp"
        android:layout_height="32dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="159dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/editTextLoginW" />

    <EditText
        android:id="@+id/editTextFirstW"
        android:layout_width="117dp"
        android:layout_height="32dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="159dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/editTextPassowrdW" />

    <EditText
        android:id="@+id/editTextLastW"
        android:layout_width="117dp"
        android:layout_height="32dp"
        android:layout_marginTop="24dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintTop_toBottomOf="@+id/editTextFirstW"
        tools:layout_editor_absoluteX="159dp" />

    <TextView
        android:id="@+id/textViewLogin"
        android:layout_width="117dp"
        android:layout_height="32dp"
        android:text="@string/textPassword"
        tools:layout_editor_absoluteX="16dp"
        tools:text="@string/textPassword"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <TextView
        android:id="@+id/textViewPassword"
        android:layout_width="117dp"
        android:layout_height="32dp"
        android:text="@string/Firstname_brute_textView"
        tools:layout_editor_absoluteX="16dp"
        tools:text="@string/Firstname_brute_textView"
        android:layout_marginTop="26dp"
        app:layout_constraintTop_toBottomOf="@+id/textViewLogin" />

    <TextView
        android:id="@+id/textViewFirst"
        android:layout_width="117dp"
        android:layout_height="32dp"
        android:text="@string/Lastname_brute_textView"
        tools:layout_editor_absoluteX="16dp"
        tools:text="@string/Lastname_brute_textView"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textViewPassword" />

    <TextView
        android:id="@+id/textViewLast"
        android:layout_width="117dp"
        android:layout_height="32dp"
        android:layout_marginTop="24dp"
        android:text="@string/Lastname_brute_textView"
        app:layout_constraintTop_toBottomOf="@+id/textViewFirst"
        tools:layout_editor_absoluteX="16dp"
        tools:text="@string/Lastname_brute_textView" />

    <Button
        android:id="@+id/buttonSave"
        android:layout_width="82dp"
        android:layout_height="56dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/save"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.979"
        app:layout_constraintHorizontal_bias="0.432" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="117dp"
        android:layout_height="32dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="16dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textViewLast" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="117dp"
        android:layout_height="32dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="159dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/editTextLastW" />

    <Button
        android:id="@+id/drapeauDE"
        android:layout_width="63dp"
        android:layout_height="56dp"
        android:background="@drawable/flag_de"
        tools:layout_editor_absoluteX="199dp"
        tools:layout_editor_absoluteY="290dp" />

    <Button
        android:id="@+id/drapeauEn"
        android:layout_width="63dp"
        android:layout_height="56dp"
        android:background="@drawable/flag_gb"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="290dp" />

    <Button
        android:id="@+id/drapeauCH"
        android:layout_width="63dp"
        android:layout_height="56dp"
        android:background="@drawable/flag_ch"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="290dp" />

</android.support.constraint.ConstraintLayout>
