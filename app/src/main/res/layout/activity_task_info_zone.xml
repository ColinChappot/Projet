<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="Task.TaskInfoZoneActivity">

    <TextView
        android:id="@+id/txtBTown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txtbTown"
        tools:text="@string/txtbTown"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/txtVTown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:text="@string/txtView"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/txtBTown" />

    <TextView
        android:id="@+id/txtBName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txtBName"
        tools:text="@string/txtBName"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/txtVTown" />

    <TextView
        android:id="@+id/txtVPlacesName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txtVPlacesName"
        tools:text="@string/txtVPlacesName"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/txtBName" />

    <TextView
        android:id="@+id/txtVSurfafce"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txtVSurface"
        tools:text="@string/txtVSurface"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/txtBSurface" />

    <TextView
        android:id="@+id/txtBSurface"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txtBSurface"
        tools:text="@string/txtBSurface"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/txtVPlacesName" />

    <ImageView
        android:id="@+id/ImgPlayGround"
        android:layout_width="153dp"
        android:layout_height="176dp"
        app:srcCompat="@mipmap/ic_launcher"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="33dp"
        android:layout_marginRight="35dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/txtBTimeToAvoid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txtBTimeToAvoid"
        tools:text="@string/txtBTimeToAvoid"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/txtVSurfafce" />

    <TextView
        android:id="@+id/txtVGPS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:text="@string/txtVGPS"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/txtBGPS" />

    <TextView
        android:id="@+id/txtBGPS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txtBGPS"
        tools:text="@string/txtBGPS"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/txtVTimeToAvoid" />

    <TextView
        android:id="@+id/txtVTimeToAvoid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:text="@string/txtVTimeToAvoid"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/txtBTimeToAvoid" />

    <TextView
        android:id="@+id/txtBInstallation"
        android:layout_width="322dp"
        android:layout_height="34dp"
        android:text="@string/txtBInstallation"
        tools:text="@string/txtBInstallation"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.318"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/listViewInstallation" />

    <ListView
        android:id="@+id/listViewInstallation"
        android:layout_width="366dp"
        android:layout_height="233dp"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="-26dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />
</android.support.constraint.ConstraintLayout>
